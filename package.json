    "babel-cli": "^6.4.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "~1.x.x",
    "es6-promise": "^3.0.2",
    "mocha": "1.21.5",
    "nock": "^9.0.2",
    "sinon": "^1.17.6"
  },
  "scripts": {
    "build": "babel src -d lib",
    "pretest": "npm run build",
