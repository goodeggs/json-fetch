{
  "name": "json-fetch",
  "version": "7.5.1",
  "description": "A wrapper around ES6 fetch to simplify interacting with JSON APIs.",
  "author": "Good Eggs <open-source@goodeggs.com>",
  "contributors": [
    "dannynelson <danny@goodeggs.com>"
  ],
  "license": "MIT",
  "keywords": [
    "fetch",
    "api",
    "json",
    "es6"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/goodeggs/json-fetch.git"
  },
  "homepage": "https://github.com/goodeggs/json-fetch",
  "bugs": "https://github.com/goodeggs/json-fetch/issues",
  "dependencies": {
    "babel-runtime": "6.26.0",
    "isomorphic-fetch": "2.2.1",
    "lodash.pick": "4.4.0",
    "promise-retry": "1.1.1"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.2",
    "babel-plugin-add-module-exports": "1.0.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-register": "6.26.0",
    "es6-promise": "4.2.5",
    "eslint": "5.9.0",
    "eslint-plugin-goodeggs": "7.1.0",
    "flow-bin": "^0.81.0",
    "goodeggs-test-helpers": "^6.1.1",
    "mocha": "5.2.0",
    "nock": "10.0.0"
  },
  "scripts": {
    "build": "rm -rf lib/ && babel src -d lib && cp src/index.js.flow lib/",
    "pretest": "yarn run build",
    "prepublishOnly": "yarn run build",
    "test": "eslint 'src/**.*.js' && flow && runtest --coverage 'src/**/test.js'"
  },
  "eslintConfig": {
    "plugins": [
      "goodeggs",
      "flowtype"
    ],
    "extends": [
      "plugin:goodeggs/goodeggs",
      "plugin:flowtype/recommended"
    ],
    "rules": {
      "no-eq-null": "off",
      "prefer-reflect": "off",
      "lodash/prefer-lodash-typecheck": "off",
      "lodash/prefer-includes": "off",
      "lodash/prefer-constant": "off"
    },
    "env": {
      "browser": true,
      "node": true
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "es2017"
    ],
    "plugins": [
      "transform-runtime",
      "transform-flow-strip-types"
    ]
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  }
}
