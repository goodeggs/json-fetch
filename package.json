{
  "name": "json-fetch",
  "version": "7.5.1",
  "description": "A wrapper around ES6 fetch to simplify interacting with JSON APIs.",
  "author": "Good Eggs <open-source@goodeggs.com>",
  "contributors": [
    "dannynelson <danny@goodeggs.com>"
  ],
  "license": "MIT",
  "keywords": [
    "fetch",
    "api",
    "json",
    "es6"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/goodeggs/json-fetch.git"
  },
  "homepage": "https://github.com/goodeggs/json-fetch",
  "bugs": "https://github.com/goodeggs/json-fetch/issues",
  "dependencies": {
    "isomorphic-fetch": "2.2.1",
    "lodash": "^4.17.10",
    "promise-retry": "1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.2",
    "@babel/core": "^7.0.0-rc.2",
    "@babel/node": "^7.0.0-rc.2",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.2",
    "@babel/preset-env": "^7.0.0-rc.2",
    "@babel/preset-typescript": "^7.0.0-rc.2",
    "@babel/register": "^7.0.0-rc.2",
    "@babel/runtime": "^7.0.0-rc.2",
    "@types/lodash.pick": "^4.4.4",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/promise-retry": "^1.1.2",
    "babel-eslint": "8.2.6",
    "babel-loader": "^7.1.5",
    "eslint": "^5.4.0",
    "eslint-plugin-goodeggs": "^5.2.0",
    "eslint-plugin-typescript": "^0.12.0",
    "flow-bin": "^0.77.0",
    "goodeggs-test-helpers": "^6.1.1",
    "mocha": "^5.2.0",
    "nock": "9.4.4",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0"
  },
  "scripts": {
    "build": "rm -rf lib/ && babel --extensions '.ts' src -d lib && tsc && cp types/index.d.ts lib/ && cp src/index.js.flow lib/",
    "pretest": "yarn run build",
    "prepublishOnly": "yarn run build",
    "test": "eslint 'src/**/*.ts' && flow && babel-node --extensions '.ts' node_modules/.bin/_mocha 'src/**/test.ts'"
  },
  "eslintConfig": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "goodeggs",
      "typescript"
    ],
    "extends": [
      "plugin:goodeggs/goodeggs"
    ],
    "rules": {
      "no-undef": "off",
      "no-eq-null": "off",
      "prefer-reflect": "off",
      "lodash/prefer-lodash-typecheck": "off",
      "lodash/prefer-includes": "off",
      "lodash/prefer-constant": "off",
      "typescript/no-unused-vars": 2
    }
  },
  "babel": {
    "presets": [
      "@babel/typescript",
      "@babel/env"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  }
}
