{
  "name": "json-fetch",
  "version": "5.1.1",
  "description": "A wrapper around ES6 fetch to simplify interacting with JSON APIs.",
  "author": "Good Eggs <open-source@goodeggs.com>",
  "contributors": [
    "dannynelson <danny@goodeggs.com>"
  ],
  "license": "MIT",
  "keywords": [
    "fetch",
    "api",
    "json",
    "es6"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/goodeggs/json-fetch.git"
  },
  "homepage": "https://github.com/goodeggs/json-fetch",
  "bugs": "https://github.com/goodeggs/json-fetch/issues",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "promise-retry": "^1.0.1"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.1.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-register": "^6.24.0",
    "es6-promise": "^3.0.2",
    "eslint": "^3.19.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-goodeggs": "^4.0.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-lodash": "^1.9.4",
    "eslint-plugin-mocha": "^4.9.0",
    "flow-bin": "^0.42.0",
    "goodeggs-test-helpers": "^1.5.0",
    "mocha": "^3.2.0",
    "nock": "^9.0.2",
    "sinon": "^1.17.6"
  },
  "scripts": {
    "build": "babel src -d lib && cp src/index.js.flow lib/",
    "pretest": "npm run build",
    "prepublish": "npm run build",
    "test": "eslint 'src/**.*.js' && flow && mocha --compilers js:babel-register"
  },
  "eslintConfig": {
    "plugins": [
      "goodeggs",
      "flowtype"
    ],
    "extends": [
      "plugin:goodeggs/goodeggs",
      "plugin:flowtype/recommended"
    ],
    "rules": {
      "no-eq-null": "off",
      "lodash/prefer-lodash-typecheck": "off",
      "lodash/prefer-constant": "off"
    },
    "env": {
      "browser": true,
      "node": true
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "es2017"
    ],
    "plugins": [
      "transform-runtime",
      "transform-flow-strip-types"
    ]
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  }
}
